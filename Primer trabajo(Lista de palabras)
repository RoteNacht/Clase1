meme_dict = {
            "CRINGE": "Algo excepcionalmente raro o embarazoso",
            "LOL": "Una respuesta común a algo gracioso",
            "ROFL": 'una respuesta a una broma',
            'SHEESH': 'ligera desaprobación',
            'CREEPY': 'aterrador, siniestro',
            'AGGRO': 'ponerse agresivo/enojado'
            }
            
word = input("Digite la palabra que desea identificar (todo en mayusculas): ")

if word in meme_dict.keys():print(f'El significado de la palabra {word} es {meme_dict[word]}')
else: print(F'La palabra {word} no fue encontrada en el diccionario')
